<% @page_title = "List of Pictures - tags #{@tag.name}" %>
<% @finish_breadcrumb = "tags > #{@tag.name}" %>

<table id="picture_list">
  <% @taggings.each_slice(3) do |sub_picture| %>
    <tr>
      <% sub_picture.each do |tagging| %>
        <%= render :partial => tagging.taggable %>
      <% end %>
    </tr>
  <% end %>
</table>

<%= will_paginate @taggings %>
